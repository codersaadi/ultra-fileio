{
	"name": "ultra-fileio",
	"version": "0.4.0",
	"description": "A powerful and flexible file upload library for Next.js with support for S3, Prisma, and Drizzle.",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./server": {
			"import": "./dist/server/index.js",
			"types": "./dist/server/index.d.ts"
		},
		"./client": {
			"import": "./dist/client/index.js",
			"types": "./dist/client/index.d.ts"
		}
	},
	"bin": {
		"ultra-fileio": "./dist/cli/index.js"
	},
	"files": ["dist"],
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"typecheck": "tsc --noEmit"
	},
	"keywords": ["nextjs", "typescript", "library"],
	"author": "Saad Bukhari <https://github.com/codersaadi>",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/codersaadi/ultra-fileio.git"
	},
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/node": "^22.19.1",
		"@types/react": "^19.0.6",
		"@types/react-dom": "^19.0.2",
		"typescript": "^5.7.2"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.939.0",
		"@aws-sdk/s3-request-presigner": "^3.939.0",
		"sharp": "^0.34.5",
		"zod": "^4.0.0"
	},
	"peerDependencies": {
		"react": ">=19.0.0",
		"react-dom": ">=19.0.0"
	}
}
